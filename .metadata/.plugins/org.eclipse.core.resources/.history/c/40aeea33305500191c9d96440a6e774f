package webproject;

import java.util.Scanner;
import java.io.*;
import javax.servlet.*;
import javax.servlet.http.*;

public class Salary extends HttpServlet{
		int salary;
		int time;
		Scanner scanner = new Scanner(System.in);
		
		System.out.println("당신의 공부시간에 따른 예상 연봉을 측정해 드립니다.");
		System.out.println("당신의 공부시간을 입력하세요 : ");
		time = scanner.nextInt();
		if(time >= 15) {
			System.out.println("당신의 초봉은 7500만원 이상입니다.");
		}else if(time >= 10) {
			System.out.println("당신의 초봉은 5000만원 이상입니다.");
		}
}


